extends layout

block content
    .main
        h1.text-center Events
    .container
        table.table.table-striped
            thead
                tr
                    th Username
                    th Event Name
                    th Type
                    th Date
                    th City
                    th State
            tbody
            each val in dataDB
                tr
                    td #{val.username}
                    td #{val.name}
                    td #{val.type}
                    td #{val.date}
                    td #{val.city}
                    td #{val.state}